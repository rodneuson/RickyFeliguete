@model RickyFeliguete.Models.AccountModels.AvailableUser

@{
    
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm())
{
    <div class="form-group">
        <label>User Name</label>
        @Html.EditorFor(model => model.UserName, new { htmlAttribute = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <label>Password</label>
        @Html.EditorFor(model => model.UserPassword, new { htmlAttibute = new { @class = "form-control" } })
    </div>
    <button type="submit" class="btn btn-primary">Login</button>
    <br />
    @Html.ActionLink("New user? Sign up here!", "Create", "AvailableUser")
}